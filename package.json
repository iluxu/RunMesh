{
  "name": "runmesh",
  "version": "0.1.0-alpha.1",
  "type": "module",
  "private": true,
  "description": "RunMesh: OpenAI-first JS/TS agent runtime that is typed, observable, and extensible.",
  "workspaces": [
    "framework/packages/*",
    "framework/apps/*"
  ],
  "scripts": {
    "build": "pnpm -r build",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,json,md}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "license": "BSL-1.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/iluxu/runmesh"
  },
  "keywords": [
    "openai",
    "agents",
    "framework",
    "typescript",
    "ai"
  ],
  "packageManager": "pnpm@8.15.1",
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.4.5",
    "vitest": "^4.0.16"
  }
}
